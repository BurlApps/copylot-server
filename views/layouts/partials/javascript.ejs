<!-- start Sentry -->
<% if(hostname != "localhost") { %>
  <script type="text/javascript">
    Raven.config('<%= sentry %>', {
      release: "<%= random %>",
      whitelistUrls: ['<%= hostname %>']
    }).install()

    <% if(user) { %>
      Raven.setUserContext({
        name: "<%= user.name  %>",
        email: "<%= user.email %>",
        id: '<%= user.id %>'
      })
    <% } %>
  </script>
<% } %>
<!-- end Sentry -->
