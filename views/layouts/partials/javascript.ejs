<% if(user) { %>
  mixpanel.identify("<%= user.id %>")
  mixpanel.people.set({
    "ID": "<%= user.id %>",
    "$name": "<%= user.name  %>",
    "$email": "<%= user.email %>",
    "Registered": true
  })
  mixpanel.register({
    "User ID": "<%= user.id %>",
    "User Name": "<%= user.name  %>",
    "User Email": "<%= user.email %>",
  });
<% } %>

mixpanel.track('Web.Page.View', {
  'Name' : "<%= siteTitle %>",
  'URL' : window.location.pathname
})

